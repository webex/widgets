{
    "extends": "../../../tsconfig.json",
    "compilerOptions": {
        "outDir": "./dist",
        "declaration": true,
        "declarationDir": "./dist/types",
        "moduleResolution": "NodeNext",
        "module": "NodeNext",
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "skipLibCheck": true,
        "noImplicitAny": false,

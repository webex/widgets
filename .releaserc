{
  "branches": [
    "master",
    {
      "name": "eft-pipeline",
      "prerelease": "alpha"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "yarn workspace @webex/cc-store --topological-dev --no-private npm version ${nextRelease.version} && yarn workspace @webex/cc-store --topological-dev --no-private npm publish --tag wxcc"
      }
    ],
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "yarn workspace @webex/cc-station-login --topological-dev --no-private npm version ${nextRelease.version} && yarn workspace @webex/cc-station-login --topological-dev --no-private npm publish --tag wxcc"
      }
    ],
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "yarn workspace @webex/cc-user-state --topological-dev --no-private npm version ${nextRelease.version} && yarn workspace @webex/cc-user-state --topological-dev --no-private npm publish --tag wxcc"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": ["CHANGELOG.md", "package.json", "packages/*/package.json"],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "@semantic-release/github"
  ]
} 
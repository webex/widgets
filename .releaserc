{
  "branches": [
    "master",
    {
      "name": "eft-pipeline",
      "prerelease": "alpha"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "node version-and-publish.js @webex/cc-store"
      }
    ],
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "node version-and-publish.js @webex/cc-station-login"
      }
    ],
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "node version-and-publish.js @webex/cc-user-state"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": ["CHANGELOG.md", "package.json", "packages/*/package.json"],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "@semantic-release/github"
  ]
} 
{
  "name": "@webex/cc-user-state",
  "description": "Webex Contact Center Widgets: User State",
  "version": "1.28.0-ccwidgets.24",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "package.json"
  ],
  "scripts": {
